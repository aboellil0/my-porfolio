{
  "buildCommand": "cd porfolio && npm install && npm run build",
  "outputDirectory": "porfolio/dist",
  "devCommand": "cd porfolio && npm run dev",
  "installCommand": "cd porfolio && npm install",
  "framework": "vite",
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
